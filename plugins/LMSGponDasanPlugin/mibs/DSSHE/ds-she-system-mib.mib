-- --------------------------------------------------------------
--       DataBase Management control subgroup
-- --------------------------------------------------------------
		sheSystemDbmControl OBJECT IDENTIFIER ::= { sheSystemDbMaintenance 3 }

		
--  This managed entity manages the control request for storage
-- of the database.
		sheSystemDbmControlRequest OBJECT-TYPE
			SYNTAX SheSystemMaintenanceControlReq
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"  Database Management commands :
				
				setSystemMaintenanceBase:
				- Set needed ftp access information for automatic DB request's.
				notifications: 
				  sheSystemDbmBaseSet
				
				backupDb : 
				- The command for a manual database backup
				  into the FEPROM of the DSLAM and to the FTP server.
				  The return value commExecStarted indicates the beginning
				  of DB storage.
				notification:
				 sheSystemDbmResult
				 
				refreshDbBackupFilesTable :
				- The DSLAM supports several
				  backup files. The command retrieves the backup files 
				  at the FTP server and provides their file names and  
				  time and date of creation in the table sheSystemDbBackFilesTable.
				notification:
				 sheSystemRefreshDbmBackupFilesTable
				 
				dbReloadSpecificFile:
				- Reload a specific file from sheSystemDbBackFilesTable
				  into the DSLAM. If successful, the CXU perform a reset 
				  for activation.
				notification:
				 sheSystemDbmResult
				
				setBackupPeriod:
				- Configure the backup period settings for automatic remote 
				  backup to ftp server and local backup to EEPROM.   
				notification:
				 sheSystemDbmBackupPeriodSet
				
				setLocalBackupAdminState:
				- Enables or disables the automatic local backup feature.
				notification:
				 sheSystemDbmLocalBackupAdminstateSet
				
				setRemoteBackupAdminState:
				- Enables or disables the automatic remote backup feature.
				notification:
				 sheSystemDbmRemoteBackupAdminstateSet
				
				"
			::= { sheSystemDbmControl 1 }

		
		sheSystemDbmControlStatus OBJECT-TYPE
			SYNTAX SheControlStatusType
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				" The status of the control request. "
			::= { sheSystemDbmControl 2 }

		
		sheSystemDbmControlTimer OBJECT-TYPE
			SYNTAX Unsigned32
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				" The maximum wait time for the manager when a DB storage
				request cannot be executed immediately (value commExecStarted
				in the result attribute sheSystemMaintenanceControlReqResult).
				The manager reads this value from the DSLAM.
				The end of request indicates the trap sheSystemTrDbBackup. "
			::= { sheSystemDbmControl 3 }

		
		sheSystemDbmControlReqResultLocal OBJECT-TYPE
			SYNTAX SheSystemMaintenanceControlReqResult
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				" The result of the local backup request. "
			::= { sheSystemDbmControl 4 }

		
		sheSystemDbmControlReqResultRemote OBJECT-TYPE
			SYNTAX SheSystemMaintenanceControlReqResult
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				" The result of the last remote backup request. "
			::= { sheSystemDbmControl 5 }

		
		sheSystemDbmControlTimeStamp OBJECT-TYPE
			SYNTAX Unsigned32
			UNITS "sec"
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				" The time stamp of the last command (end of command). "
			::= { sheSystemDbmControl 6 }

		
		sheSystemDbmControlServerAddr OBJECT-TYPE
			SYNTAX OCTET STRING (SIZE (0..20))
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"Address of DB FTP server (dotted notation)."
			::= { sheSystemDbmControl 7 }

		
		sheSystemDbmControlPathName OBJECT-TYPE
			SYNTAX OCTET STRING (SIZE (0..128))
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"Path name of DB backup files on FTP server. 
				This path should be set relative to the ftp-server root.
				Use only linux notation ( / ). 
				
				Spaces in pathnames are not allowed, because of possible problems with
				different FTP implementations (client and/or server)!!"
			::= { sheSystemDbmControl 8 }

		
		sheSystemDbmControlUsrName OBJECT-TYPE
			SYNTAX OCTET STRING (SIZE (0..20))
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"Username for DB backup FTP access.                                                             
				
				Read-access to this attribute will deliver 20 times sign '-'  "
			::= { sheSystemDbmControl 9 }

		
		sheSystemDbmControlPassword OBJECT-TYPE
			SYNTAX OCTET STRING (SIZE (0..20))
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"Password for DB backup FTP access.                                                              
				
				Read-access to this attribute will deliver 20 times sign '-'  "
			::= { sheSystemDbmControl 10 }

		
		sheSystemDbmControlRemoteBackupAdminState OBJECT-TYPE
			SYNTAX SheSystemAdminState
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"This set the current usage of automatic remote db backup.
				The following states are supported:
				
				unlocked(1)  -> remote db backup handling used
				locked(2)    -> remote db backup handling not used (default)"
			::= { sheSystemDbmControl 11 }

		
		sheSystemDbmControlBackupStartTime OBJECT-TYPE
			SYNTAX Unsigned32 (0..1439)
			UNITS "minute"
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"Start time basis for remote DB backup after system restart, backup timer 
				reconfigurations or system clock reset.
				The first backup will be started at this time + sheSystemDbmBackupPeriod and
				will then be repeated each sheSystemDbmBackupPeriod.
				Range: 0 (0 am) .. 1439 (23.59 pm).
				Reminder: NE uses always a UTC time base.
				"
			::= { sheSystemDbmControl 12 }

		
		sheSystemDbmControlBackupPeriod OBJECT-TYPE
			SYNTAX Unsigned32 (1..48)
			UNITS "hour"
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"Interval between two remote backup operations in hours. 
				Backup is started only if there were relevant configuration
				changes during the last period."
			::= { sheSystemDbmControl 13 }

		
		sheSystemDbmControlBackupWaitingPeriod OBJECT-TYPE
			SYNTAX Unsigned32 (1..59)
			UNITS "minute"
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"Waiting period in minutes before next local backup, in case of configuration changes."
			::= { sheSystemDbmControl 14 }

		
		sheSystemDbmControlBackupMaxFileNumber OBJECT-TYPE
			SYNTAX Unsigned32 (1..32)
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"This number describes the maximum set of different backup files on the remote ftp site.
				"
			::= { sheSystemDbmControl 15 }

		
		sheSystemDbmControlLocalBackupAdminState OBJECT-TYPE
			SYNTAX SheSystemAdminState
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"This value administrate the automatic local db backup after sheSystemDbmBackupWaitingPeriod
				in case of configuration actions.
				Default: unlocked  (automatic local backup is enabled)"
			::= { sheSystemDbmControl 16 }

		
		sheSystemDbmControlBackupFileIndex OBJECT-TYPE
			SYNTAX Integer32 (1..32)
			MAX-ACCESS read-write
			STATUS current
			DESCRIPTION
				"The index selects the DB backup file in the sheSystemDbBackupFilesTable which shall be loaded
				into the DSLAM from the FTP server with user command dbReloadSpecificFile. "
			::= { sheSystemDbmControl 17 }

		
		sheSystemDbMaintenanceNotifications OBJECT IDENTIFIER ::= { sheSystemDbMaintenance 4 }

		
		sheSystemDbmBaseSet NOTIFICATION-TYPE
			OBJECTS { sheSystemDbmControlStatus, sheSystemDbmControlTimeStamp, sheSystemDbmControlReqResultRemote, sheSystemDbmControlServerAddr, sheSystemDbmControlPathName
				 }
			STATUS current
			DESCRIPTION 
				"Configuration trap for remote DB basic access settings."
			::= { sheSystemDbMaintenanceNotifications 1 }

		
		sheSystemDbmResult NOTIFICATION-TYPE
			OBJECTS { sheSystemDbmControlTimeStamp, sheSystemDbmControlReqResultLocal, sheSystemDbmControlReqResultRemote, sheSystemDbmControlStatus }
			STATUS current
			DESCRIPTION 
				"Indicates the end and the result of the database backup or restore."
			::= { sheSystemDbMaintenanceNotifications 3 }

		
		sheSystemDbmBackupFilesTableRefreshed NOTIFICATION-TYPE
			OBJECTS { sheSystemDbmControlTimeStamp, sheSystemDbmControlStatus, sheSystemDbmControlReqResultRemote }
			STATUS current
			DESCRIPTION 
				"Indicates end of user command refreshDbmBackupFilesTable."
			::= { sheSystemDbMaintenanceNotifications 4 }

		
		sheSystemDbmBackupPeriodSet NOTIFICATION-TYPE
			OBJECTS { sheSystemDbmControlTimeStamp, sheSystemDbmControlStatus, sheSystemDbmBackupMaxFileNumber, sheSystemDbmControlReqResultLocal, sheSystemDbmBackupPeriod, 
				sheSystemDbmBackupStartTime, sheSystemDbmBackupWaitingPeriod }
			STATUS current
			DESCRIPTION 
				"Settings for automatic backup period are set."
			::= { sheSystemDbMaintenanceNotifications 5 }

		
		sheSystemDbmLocalBackupAdminstateSet NOTIFICATION-TYPE
			OBJECTS { sheSystemDbmControlTimeStamp, sheSystemDbmControlReqResultLocal, sheSystemDbmControlStatus, sheSystemDbmLocalBackupAdminState }
			STATUS current
			DESCRIPTION 
				"Configuration notification for local db backup."
			::= { sheSystemDbMaintenanceNotifications 6 }

		
		sheSystemDbmRemoteBackupAdminstateSet NOTIFICATION-TYPE
			OBJECTS { sheSystemDbmControlTimeStamp, sheSystemDbmControlStatus, sheSystemDbmControlReqResultRemote, sheSystemDbmControlRemoteBackupAdminState }
			STATUS current
			DESCRIPTION 
				"Configuration notification for local db backup."
			::= { sheSystemDbMaintenanceNotifications 7 }

		
		sheSystemDbMaintenanceAlarms OBJECT IDENTIFIER ::= { sheSystemDbMaintenance 5 }

		
		sheSystemLocalDbInvalid NOTIFICATION-TYPE
			OBJECTS { sheAlarmTimeAndDate, sheAlarmReportingSource, sheAlarmReportingSourceType, sheAlarmSeverity }
			STATUS current
			DESCRIPTION 
				"Alarm will be set if the local DB is invalid because of failures during
				read/write local DB or if the local DB was not updated yet after a 
				configuration sequence."
			::= { sheSystemDbMaintenanceAlarms 1 }

		
		sheSystemRemoteDbInvalid NOTIFICATION-TYPE
